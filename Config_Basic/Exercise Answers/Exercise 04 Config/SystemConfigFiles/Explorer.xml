<explorer version="1.1" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/explorer.xsd">
	<systemInformation>
		<systemCaption>$EXPLORER_SYSTEMCAPTION$</systemCaption>
		<!--<systemHelpFile>%REGION_HOME%/Help.pdf</systemHelpFile>-->
		<systemHelpUrl>https://publicwiki.deltares.nl/display/FEWSDOC/Home</systemHelpUrl>
	</systemInformation>
	<rollingBarrelOptions>
		<type>not_automatic</type>
	</rollingBarrelOptions>
	<globalDatum>mAOD</globalDatum>
	<map>
		<geoDatum>WGS 1984</geoDatum>
		<projection>mercator</projection>
		<defaultExtent id="UK">
			<left>-13.673693</left>
			<right>8.055557</right>
			<top>58.989624</top>
			<bottom>49.716423</bottom>
		</defaultExtent>
		<extraExtent id="Europe">
			<left>-25.868914</left>
			<right>61.357376</right>
			<top>70.57443</top>
			<bottom>34.599728</bottom>
		</extraExtent>
		<extraExtent id="Isle of Man">
			<left>-5.19</left>
			<right>-3.87</right>
			<top>54.50</top>
			<bottom>53.94</bottom>
		</extraExtent>
		<scaleBarVisible>true</scaleBarVisible>
		<northArrowVisible>true</northArrowVisible>
		<labelsVisible>true</labelsVisible>
		<backgroundColor>azure</backgroundColor>
		<openStreetMapLayer id="Landscape">
			<url>http://tile.thunderforest.com/landscape</url>
			<visible>true</visible>
			<cacheDir>$REGION_HOME$/LandscapeTiles</cacheDir>
		</openStreetMapLayer>
		<esriShapeLayer id="Rest of Europe">
			<file>%REGION_HOME%/map/Europe_withoutGB.shp</file>
			<geoDatum>WGS 1984</geoDatum>
			<visible>true</visible>
			<fillColor>goldenrod</fillColor>
			<opaquenessPercentage>20</opaquenessPercentage>
		</esriShapeLayer>
		<esriShapeLayer id="United Kingdom">
			<description>United Kingdom Coastline</description>
			<file>%REGION_HOME%/map/coastline_polygon.shp</file>
			<geoDatum>Ordnance Survey Great Britain 1936</geoDatum>
			<visible>true</visible>
			<lineColor>gray50</lineColor>
			<fillColor>gold</fillColor>
			<opaquenessPercentage>40</opaquenessPercentage>
		</esriShapeLayer>
		<gridLayer/>
		<!--<esriShapeLayer id="Rest of Europe">
			<file>%REGION_HOME%/map/Europe_withoutGB.shp</file>
			<geoDatum>WGS 1984</geoDatum>
			<visible>true</visible>
			<lineColor>black</lineColor>
			<fillColor>gray</fillColor>
			<opaquenessPercentage>0</opaquenessPercentage>
		</esriShapeLayer>
		<esriShapeLayer id="Coastline_reduced">
			<description>United Kingdom Coastline</description>
			<file>%REGION_HOME%/map/coastline_polygon.shp</file>
			<geoDatum>Ordnance Survey Great Britain 1936</geoDatum>
			<visible>true</visible>
		</esriShapeLayer>-->
		<!--
    <esriShapeLayer id="Countries">
      <description>Countries</description>
      <file>%REGION_HOME%/Map/Countries.shp</file>
      <geoDatum>WGS 1984</geoDatum>
      <visible>true</visible>
      <fillColor>gray</fillColor>
    </esriShapeLayer>
    -->
	</map>
	<explorerTasks>
		<explorerTask name="Shortcuts">
			<arguments>shortcuts</arguments>
			<taskClass>nl.wldelft.fews.gui.plugin.timeseries.TimeSeriesDialog</taskClass>
			<toolbarTask>false</toolbarTask>
			<menubarTask>false</menubarTask>
			<accelerator>ENTER</accelerator>
			<doubleClickLocationEnabled>true</doubleClickLocationEnabled>
		</explorerTask>
		<explorerTask name="Map Display">
			<iconFile>%FEWSDIR%/icons/splitmap.gif</iconFile>
			<mnemonic>a</mnemonic>
			<arguments>MapDisplay</arguments>
			<taskClass>nl.wldelft.fews.gui.plugin.map.MapDisplay</taskClass>
			<toolbarTask>true</toolbarTask>
			<menubarTask>true</menubarTask>
			<accelerator>ctrl A</accelerator>
		</explorerTask>
		<explorerTask name="Data Editor">
			<mnemonic>E</mnemonic>
			<arguments>table</arguments>
			<taskClass>nl.wldelft.fews.gui.plugin.timeseries.TimeSeriesDialog</taskClass>
			<toolbarTask>false</toolbarTask>
			<menubarTask>true</menubarTask>
			<accelerator>ctrl E</accelerator>
		</explorerTask>
		<explorerTask name="Plots">
			<iconFile>graph.gif</iconFile>
			<mnemonic>D</mnemonic>
			<taskClass>nl.wldelft.fews.gui.plugin.timeseries.PlotsTimeSeriesDialog</taskClass>
			<toolbarTask>true</toolbarTask>
			<menubarTask>true</menubarTask>
			<allowMultipleInstances>true</allowMultipleInstances>
			<accelerator>ctrl D</accelerator>
			<loadAtStartup>true</loadAtStartup>
		</explorerTask>
		<explorerTask name="Spatial Display">
			<mnemonic>p</mnemonic>
			<arguments>GridDisplay</arguments>
			<taskClass>nl.wldelft.fews.gui.plugin.grid.GridDisplay</taskClass>
			<toolbarTask>true</toolbarTask>
			<menubarTask>true</menubarTask>
			<allowMultipleInstances>true</allowMultipleInstances>
			<accelerator>ctrl P</accelerator>
		</explorerTask>
		<explorerTask name="Manual Forecast">
			<iconFile>""</iconFile>
			<mnemonic>M</mnemonic>
			<taskClass>nl.wldelft.fews.gui.plugin.manualforecast.ManualForecastDialog</taskClass>
			<toolbarTask>true</toolbarTask>
			<menubarTask>true</menubarTask>
			<accelerator>ctrl N</accelerator>
		</explorerTask>
		<explorerTask name="Forecast Management">
			<iconFile>""</iconFile>
			<mnemonic>F</mnemonic>
			<taskClass>nl.wldelft.fews.gui.plugin.forecast.ForecastDialog</taskClass>
			<toolbarTask>true</toolbarTask>
			<menubarTask>true</menubarTask>
			<accelerator>ctrl F</accelerator>
		</explorerTask>
		<explorerTask name="System Monitor">
			<iconFile>""</iconFile>
			<mnemonic>S</mnemonic>
			<taskClass>nl.wldelft.fews.gui.plugin.systemmonitor.SystemMonitorDialog</taskClass>
			<toolbarTask>true</toolbarTask>
			<menubarTask>true</menubarTask>
			<accelerator>ctrl S</accelerator>
		</explorerTask>
		<explorerTask name="Thresholds Display">
			<arguments>ThresholdsDisplay</arguments>
			<taskClass>nl.wldelft.fews.gui.plugin.thresholdoverview.ThresholdOverviewDisplay</taskClass>
			<toolbarTask>true</toolbarTask>
			<menubarTask>true</menubarTask>
			<accelerator>ctrl T</accelerator>
		</explorerTask>
	</explorerTasks>
	<statusBarConfig>
		<description>Standard date-time format</description>
		<dateTimeFormat>HH:mm:ss</dateTimeFormat>
		<timeGMT>true</timeGMT>
		<timeCurrent>true</timeCurrent>
		<timeLastRefresh>true</timeLastRefresh>
		<statusOfSystem>true</statusOfSystem>
		<mouseCoordinates>true</mouseCoordinates>
		<userName>true</userName>
	</statusBarConfig>
	<dateTime>
		<timeZoneName>GMT</timeZoneName>
		<dateTimeFormat>dd-MM-yyyy HH:mm:ss</dateTimeFormat>
		<cardinalTimeStep unit="hour" divider="1" multiplier="1"/>
	</dateTime>
	<logPanelConfig>
		<clientFilter>
			<clientId>Stand alone</clientId>
			<logFilter>
				<level>INFO</level>
				<eventType>system</eventType>
			</logFilter>
			<logFilter>
				<level>INFO</level>
				<eventType>debug</eventType>
			</logFilter>
		</clientFilter>
		<clientFilter>
			<clientId>Operator Client</clientId>
			<logFilter>
				<level>INFO</level>
				<eventType>system</eventType>
			</logFilter>
			<logFilter>
				<level>INFO</level>
				<eventType>debug</eventType>
			</logFilter>
		</clientFilter>
	</logPanelConfig>
	<panelSizes>
		<loggingPanelSize>15</loggingPanelSize>
		<listsPanelSize>20</listsPanelSize>
		<filterListSize>20</filterListSize>
		<locationListSize>40</locationListSize>
		<parameterListSize>25</parameterListSize>
		<forecastListSize>15</forecastListSize>
	</panelSizes>
	<loginEarliestAllowedLocalDataStoreCreationDate date="2006-05-01" time="08:00:00"/>
</explorer>
