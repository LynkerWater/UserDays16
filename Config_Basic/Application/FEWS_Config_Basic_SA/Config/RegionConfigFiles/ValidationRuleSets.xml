<?xml version="1.0" encoding="UTF-8"?>
<validationRuleSets version="1.1" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/validationRuleSets.xsd">
  <validationRuleSet validationRuleSetId="London Example rule" timeZone="0">
    <extremeValues>
      <hardMax constantLimit="9999"/>
      <softMax constantLimit="50"/>
    </extremeValues>
    <timeSeriesSet>
      <moduleInstanceId>ImportTelemetry</moduleInstanceId>
      <valueType>scalar</valueType>
      <parameterId>H.obs</parameterId>
      <locationId>London_hyd</locationId>
      <timeSeriesType>external historical</timeSeriesType>
      <timeStep unit="minute" multiplier="15"/>
      <readWriteMode>add originals</readWriteMode>
    </timeSeriesSet>
  </validationRuleSet>
  <validationRuleSet validationRuleSetId="Hydro1" timeZone="0">
    <extremeValues>
      <hardMax constantLimit="1.5"/>
