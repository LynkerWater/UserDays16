				<condition>
					<columnId>Answer</columnId>
					<rowId>7</rowId>
					<ifForecastPublicationTime>
						<operator>before</operator>
						<value date="2012-09-19" time="18:30:00"/>
					</ifForecastPublicationTime>
				</condition>
				<file>call_return_1.jpg</file>
			</frame>
		</story>
